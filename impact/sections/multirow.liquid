{{ 'section-multirow.css' | asset_url | stylesheet_tag }}

{% style %}
  {% render 'section-padding' %}
  {% render 'section-margin' %}
{% endstyle %}

<div class="multirow color-{{ section.settings.color_scheme }} background section-{{ section.id }}-padding multirow--{% if section.settings.section_type == 'separators' %}separators{% elsif section.settings.section_type == 'transparent' %}transparent{% elsif section.settings.section_type == 'solid' %}solid{% endif %}">
