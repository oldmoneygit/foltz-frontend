        <div
          class="multirow-card multirow-items__item multirow-card--{{ section.settings.image_alignment }} multirow-card--{{ section.settings.image_width }} multirow-card-{{ block.id }}"
          {{ block.shopify_attributes }}
        >
          <div class="multirow-card__info">
            {%- if block.settings.icon != 'none' -%}
              <div
                class="
                  multirow-card__image-wrapper multirow-card__image-wrapper--icon multirow-card__image-wrapper--{{- section.settings.icon_size }} {% if section.settings.box_shadow %}multirow-card__image-wrapper--shadow
                  {% endif %} multirow-card__image-wrapper--{{- section.settings.icon_style }} multirow-card__image-wrapper--{{- block.settings.icon_fill_type }} color-{{ block.settings.icon_color_scheme }}
                "
              >
                <i
                  class="icon icon-pack icon-{{ block.settings.icon }} {% if section.settings.icon_style == 'colored' and block.settings.icon_fill_type == 'gradient' %} text-gradient {% endif %}"
                ></i>
              </div>
            {%- endif -%}
            {%- if block.settings.heading != blank -%}
              <h3 class="multirow-card__heading {{ section.settings.card_heading_size }} title--{{ block.settings.heading_line }} 
                {% if block.settings.word_break and block.settings.heading_line != 'none' %} title--word_break {% endif %}"
                style="--color-line: {{ block.settings.line_color }}">
                {{ block.settings.heading }}
              </h3>
            {%- endif -%}
            {%- if block.settings.text != blank -%}
              <div class="multirow-card__text body-large">{{ block.settings.text }}</div>
            {%- endif -%}
            {%- if block.settings.show_button != blank -%}
              {% render 'button',
                link: block.settings.button_link,
                label: block.settings.button_label,
                style: block.settings.button_style,
                class: 'multirow-card'
              %}
            {% endif %}
          </div>

          {% if block.settings.show_video == true %}
            {% render 'video', object: block, class: 'multirow' %}
          {% elsif block.settings.image != blank %}
            <div class="multirow-card__image-wrapper multirow-card__image-wrapper--image multirow-card__image-wrapper--{{ section.settings.image_ratio }} multirow-card__image-wrapper--{{ section.settings.image_width }}">
              {% liquid
                assign aspect_ratio = block.settings.image.aspect_ratio

                case section.settings.image_ratio
                  when 'portrait'
                    assign image_ratio = 1.4
                  when 'square'
                    assign image_ratio = 1
                  else
                    assign image_ratio = 0.5
                endcase
              %}

              {% if aspect_ratio > image_ratio and block.settings.image.width > block.settings.image.height %}
                {% capture sizes %}
									calc((100vw - 40px) * {{ aspect_ratio }} * {{ image_ratio }})
								{% endcapture %}
              {% elsif aspect_ratio > image_ratio and block.settings.image.width < block.settings.image.height %}
                {% capture sizes %}
									calc((100vw - 40px) / {{ aspect_ratio }} * {{ image_ratio }})
								{% endcapture %}
              {% else %}
                {% capture sizes %} 
									calc((100vw - 40px))
								{% endcapture %}
              {% endif %}
              {% liquid
                assign height = block.settings.image.height
                assign width = block.settings.image.width
              %}
              {%- capture style -%}object-fit: {{ section.settings.object_fit }};{%- endcapture -%}
              {{
                block.settings.image
                | image_url: width: block.settings.image.width
                | image_tag:
                  loading: 'lazy',
                  sizes: sizes,
                  widths: '360, 535, 600, 720, 940, 1066, 1200, 1400, 1600, 1800, 2000, 2200, 2400, 2600, 2800, 3000, 3200, 3400, 3600, 3800',
                  height: height,
                  width: width,
                  style: style
              }}
            </div>
          {% endif %}
        </div>
      {% endfor %}
    </div>
  </div>
</div>

