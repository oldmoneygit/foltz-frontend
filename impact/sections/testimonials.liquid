                            {% if block.settings.slide_name != blank or block.settings.slide_position != blank %}
                              <div class="testimonials__author-info">
                                {% if block.settings.slide_name != blank %}
                                  <div class="testimonials__author-name {% if section.settings.layout == 'full_slider' or section.settings.layout == 'image_left_slider' %} body-large {% endif %}">
                                    {{ block.settings.slide_name }}
                                  </div>
                                {% endif %}
                                {% if block.settings.slide_position != blank %}
                                  <div class="testimonials__author-position body-small">
                                    {{ block.settings.slide_position }}
                                  </div>
                                {% endif %}
                              </div>
                            {% endif %}
                          </div>
                        {% endif %}
                      </div>
                    </div>
                  </div>
                {% endif %}
              {% endfor %}
            {% endif %}
            {% if section.blocks.size > 6 %}
              </div>
            {% endif %}
          </div>
        </div>

        <div class="testimonials__marquee-wrapper_mobile {% if section.blocks.size > 3 %} testimonials__marquee-wrapper_mobile-full {% endif %}">
          <div class="testimonials__marquee {% if section.blocks.size > 3 %} testimonials__marquee-swiper_mobile swiper {% endif %}">
            {% if section.blocks.size > 3 %}
              <div class="swiper-wrapper">
            {% endif %}
            {%- for block in section.blocks -%}
              <div class="swiper-slide">
                <div
                  class="testimonials__slide testimonials__slide--{{ content_align }} color-{{ section.settings.slide_color_scheme }}"
                  {{ block.shopify_attributes }}
                >
                  <div class="testimonials__item">
                    <div class="testimonials__rating">
                      <div class="testimonials__stars count-stars-{{ block.settings.rating }}">
                        <i class="icon-star"></i>
                      </div>
                      <div class="testimonials__rating-count body-small">
                        {{ block.settings.rating | append: '.00' }}
                      </div>
                    </div>

                    {% if block.settings.slide_text != blank %}
                      <p class="testimonials__text {{ section.settings.slide_text_size }} {% if section.settings.slide_text_size == 'recommended' %} {{ text_size }} {% endif %}">
                        {{ block.settings.slide_text | escape }}
                      </p>
                    {% endif %}

                    {% if block.settings.image != blank
                      or block.settings.slide_name != blank
                      or block.settings.slide_position != blank
                    %}
                      <div class="testimonials__author">
                        {% if block.settings.image != blank %}
                          <div class="testimonials__author-icon">
                            {{
                              block.settings.image
                              | image_url: width: block.settings.image.width
                              | image_tag: loading: 'lazy', widths: '70, 140'
                            }}
                          </div>
                        {% endif %}
                        {% if block.settings.slide_name != blank or block.settings.slide_position != blank %}
                          <div class="testimonials__author-info">
                            {% if block.settings.slide_name != blank %}
                              <div class="testimonials__author-name {% if section.settings.layout == 'full_slider' or section.settings.layout == 'image_left_slider' %} body-large {% endif %}">
                                {{ block.settings.slide_name }}
                              </div>
                            {% endif %}
                            {% if block.settings.slide_position != blank %}
                              <div class="testimonials__author-position body-small">
                                {{ block.settings.slide_position }}
                              </div>
                            {% endif %}
                          </div>
                        {% endif %}
                      </div>
                    {% endif %}
                  </div>
                </div>
              </div>
            {% endfor %}

            {% if section.blocks.size > 3 and section.blocks.size < 6 %}
              {%- for block in section.blocks -%}
                <div class="swiper-slide">
                  <div
                    class="testimonials__slide testimonials__slide--{{ content_align }} color-{{ section.settings.slide_color_scheme }}"
                    {{ block.shopify_attributes }}
                  >
                    <div class="testimonials__item">
                      <div class="testimonials__rating">
                        <div class="testimonials__stars count-stars-{{ block.settings.rating }}">
                          <i class="icon-star"></i>
                        </div>
                        <div class="testimonials__rating-count body-small">
                          {{ block.settings.rating | append: '.00' }}
                        </div>
                      </div>

                      {% if block.settings.slide_text != blank %}
                        <p class="testimonials__text {{ section.settings.slide_text_size }} {% if section.settings.slide_text_size == 'recommended' %} {{ text_size }} {% endif %}">
                          {{ block.settings.slide_text | escape }}
                        </p>
                      {% endif %}

                      {% if block.settings.image != blank
                        or block.settings.slide_name != blank
                        or block.settings.slide_position != blank
                      %}
                        <div class="testimonials__author">
                          {% if block.settings.image != blank %}
                            <div class="testimonials__author-icon">
                              {{
                                block.settings.image
                                | image_url: width: block.settings.image.width
                                | image_tag: loading: 'lazy', widths: '70, 140'
                              }}
                            </div>
                          {% endif %}
                          {% if block.settings.slide_name != blank or block.settings.slide_position != blank %}
                            <div class="testimonials__author-info">
                              {% if block.settings.slide_name != blank %}
                                <div class="testimonials__author-name {% if section.settings.layout == 'full_slider' or section.settings.layout == 'image_left_slider' %} body-large {% endif %}">
                                  {{ block.settings.slide_name }}
                                </div>
                              {% endif %}
                              {% if block.settings.slide_position != blank %}
                                <div class="testimonials__author-position body-small">
                                  {{ block.settings.slide_position }}
                                </div>
                              {% endif %}
                            </div>
                          {% endif %}
                        </div>
                      {% endif %}
                    </div>
                  </div>
                </div>
              {% endfor %}
            {% endif %}

            {% if section.blocks.size > 3 %}
              </div>
            {% endif %}
          </div>
        </div>
      {% endif %}
    </div>
  </div>
</div>

