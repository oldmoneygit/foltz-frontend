{{ 'section-multicolumn.css' | asset_url | stylesheet_tag }}

{% style %}
   {% render 'section-padding' %}
   {% render 'section-margin' %}

  @media screen and (min-width: 576px) and (max-width: 989px) {
    .multicolumn--separators .multicolumn-card:nth-child(2n - 1) {
      padding-left: 0!important;
      border: none;
    }
  }


  @media screen and (min-width: 990px) {
    .multicolumn--separators .multicolumn-card:nth-child({{ section.settings.multicolumn_to_show }}n + 1) {
      padding-left: 0!important;
      border: none;
    }
  }
{% endstyle %}

<div
  class="
    multicolumn color-{{ section.settings.color_scheme }} background section-{{ section.id }}-padding
    {% if section.settings.enable_border %} section-border {% if section.settings.border_full %} section-border-full {% endif %} {% endif %}
    multicolumn-gap--{{ section.settings.gap }}
    multicolumn--{% if section.settings.section_type == 'separators' %}separators{% elsif section.settings.section_type == 'transparent' %}transparent{% elsif section.settings.section_type == 'solid' %}solid{% endif %}
  "
>
  <div class="container">
    {% render 'section-text', section: section, class: 'multicolumn', text_align: section.settings.text_align %}

    <div class="multicolumn-list__wrapper multicolumn-list__wrapper--{{ section.settings.multicolumn_to_show }} multicolumn-list__wrapper--static content">
      {% for block in section.blocks %}
        <div
          class="multicolumn-card"
          {{ block.shopify_attributes }}
        >
          {%- if block.settings.icon != 'none' -%}
            <div
              class="
                multicolumn-card__image-wrapper multicolumn-card__image-wrapper--icon multicolumn-card__image-wrapper--{{- section.settings.icon_size }} {% if section.settings.box_shadow %}multicolumn-card__image-wrapper--shadow
                {% endif %} multicolumn-card__image-wrapper--{{- section.settings.icon_style }} multicolumn-card__image-wrapper--{{- block.settings.icon_fill_type }} color-{{ block.settings.icon_color_scheme }}
              "
            >
              <i
                class="icon icon-pack icon-{{ block.settings.icon }} {% if section.settings.icon_style == 'colored' and block.settings.icon_fill_type == 'gradient' %} text-gradient {% endif %}"
              ></i>
            </div>
          {%- endif -%}
          {%- if block.settings.image != blank -%}
            <div class="multicolumn-card__image-wrapper multicolumn-card__image-wrapper--image multicolumn-card__image-wrapper--{{ section.settings.image_width }} multicolumn-card__image-wrapper--{{ section.settings.image_ratio }}">
              {% liquid
                assign aspect_ratio = block.settings.image.aspect_ratio

                case section.settings.image_ratio
                  when 'portrait'
                    assign image_ratio = 1.4
                  when 'square'
                    assign image_ratio = 1
                  else
                    assign image_ratio = 0.5
                endcase
              %}

              {% if aspect_ratio > image_ratio and block.settings.image.width > block.settings.image.height %}
                {% case section.settings.multicolumn_to_show %}
                  {% when 4 %}
                    {% capture sizes %}
											(min-width: 1360px) calc((100vw - 128px) / 4 * {{ aspect_ratio }} * {{ image_ratio }}), (min-width: 990px) calc((100vw - 40px) / 3 * {{ aspect_ratio }} * {{ image_ratio }}), (min-width: 576px) calc((100vw - 40px) / 2 * {{ aspect_ratio }} * {{ image_ratio }}), calc((100vw - 40px) * {{ aspect_ratio }} * {{ image_ratio }})
										{% endcapture %}
                  {% when 3 %}
                    {% capture sizes %}
											(min-width: 990px) calc((100vw - 40px) / 3 * {{ aspect_ratio }} * {{ image_ratio }}), (min-width: 576px) calc((100vw - 40px) / 2 * {{ aspect_ratio }} * {{ image_ratio }}), calc((100vw - 40px) * {{ aspect_ratio }} * {{ image_ratio }})
										{% endcapture %}
                  {% when 2 %}
                    {% capture sizes %}
											(min-width: 576px) calc((100vw - 40px) / 2 * {{ aspect_ratio }} * {{ image_ratio }}), calc((100vw - 40px) * {{ aspect_ratio }} * {{ image_ratio }})
										{% endcapture %}
                  {% else %}
                    {% capture sizes %}
											calc((100vw - 40px) * {{ aspect_ratio }} * {{ image_ratio }})
										{% endcapture %}
                {% endcase %}
              {% elsif aspect_ratio > image_ratio and block.settings.image.width < block.settings.image.height %}
                {% case section.settings.multicolumn_to_show %}
                  {% when 4 %}
                    {% capture sizes %}
											(min-width: 1360px) calc((100vw - 128px) / 4 / {{ aspect_ratio }}) * {{ image_ratio }}, (min-width: 990px) calc((100vw - 40px) / 3 / {{ aspect_ratio }} * {{ image_ratio }}), (min-width: 576px) calc((100vw - 40px) / 2 / {{ aspect_ratio }} * {{ image_ratio }}), calc((100vw - 40px) / {{ aspect_ratio }} * {{ image_ratio }})
										{% endcapture %}
                  {% when 3 %}
                    {% capture sizes %}
											(min-width: 990px) calc((100vw - 40px) / 3 / {{ aspect_ratio }} * {{ image_ratio }}), (min-width: 576px) calc((100vw - 40px) / 2 / {{ aspect_ratio }} * {{ image_ratio }}), calc((100vw - 40px) / {{ aspect_ratio }} * {{ image_ratio }})
										{% endcapture %}
                  {% when 2 %}
                    {% capture sizes %}
											(min-width: 576px) calc((100vw - 40px) / 2 / {{ aspect_ratio }} * {{ image_ratio }}), calc((100vw - 40px) / {{ aspect_ratio }} * {{ image_ratio }})
										{% endcapture %}
                  {% else %}
                    {% capture sizes %}
											calc((100vw - 40px) / {{ aspect_ratio }} * {{ image_ratio }})
										{% endcapture %}
                {% endcase %}
              {% else %}
                {% case section.settings.multicolumn_to_show %}
                  {% when 4 %}
                    {% capture sizes %} 
											(min-width: 1360px) calc((100vw - 128px) / 4), (min-width: 990px) calc((100vw - 40px) / 3), (min-width: 576px) calc((100vw - 40px) / 2), calc((100vw - 40px))
										{% endcapture %}
                  {% when 3 %}
                    {% capture sizes %} 
											(min-width: 990px) calc((100vw - 40px) / 3), (min-width: 576px) calc((100vw - 40px) / 2), calc((100vw - 40px))
										{% endcapture %}
                  {% when 2 %}
                    {% capture sizes %} 
											(min-width: 576px) calc((100vw - 40px) / 2), calc((100vw - 40px))
										{% endcapture %}
                  {% else %}
                    {% capture sizes %} 
											calc((100vw - 40px))
										{% endcapture %}
                {% endcase %}
              {% endif %}
              {% liquid
                assign height = block.settings.image.height
                assign width = block.settings.image.width
              %}
              {%- capture style -%}object-fit: {{ section.settings.object_fit }};{%- endcapture -%}
              {{
                block.settings.image
                | image_url: width: block.settings.image.width
                | image_tag:
                  loading: 'lazy',
                  sizes: sizes,
                  widths: '360, 535, 600, 720, 940, 1066, 1200, 1400, 1600, 1800, 2000, 2200, 2400, 2600, 2800, 3000, 3200, 3400, 3600, 3800',
                  height: height,
                  width: width,
                  style: style
              }}
            </div>
          {%- endif -%}

          <div class="multicolumn-card__info">
            {%- if block.settings.heading != blank -%}
              <h3 class="multicolumn-card__heading {{ section.settings.card_heading_size }}">
                {{ block.settings.heading | escape }}
              </h3>
            {%- endif -%}
            {%- if block.settings.text != blank -%}
              <div class="multicolumn-card__text">{{ block.settings.text }}</div>
            {%- endif -%}
            {%- if block.settings.show_button != blank -%}
              {% render 'button',
                link: block.settings.button_link,
                label: block.settings.button_label,
                style: block.settings.button_style,
                class: 'multicolumn-card'
              %}
            {% endif %}
          </div>
        </div>
      {% endfor %}
    </div>
  </div>
</div>

