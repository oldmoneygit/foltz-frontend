{{ 'section-animated-image-collage.css' | asset_url | stylesheet_tag }}
{% if section.settings.enable_subscribe %}
  {{ 'component-subscription.css' | asset_url | stylesheet_tag }}
{% endif %}
<script src="{{ 'animated-image-collage.js' | asset_url }}" defer="defer"></script>

{% style %}
   {% render 'section-margin' %}
  {% if section.settings.section_type == 'bottom' %}
  	{% render 'section-padding' %}
  {% endif %}
{% endstyle %}

<div
  class="aic aic--{% if section.settings.section_type == 'background' %}background color-{{ section.settings.color_scheme }} background {% elsif section.settings.section_type == 'right' %}right{% elsif section.settings.section_type == 'bottom' %}bottom{% endif %} {% if section.settings.full_width != true %}container{% endif %} {% if section.settings.header_overlay %}section--has-overlay {% if section.settings.header_transparent %} section--header-light {% endif %} {% endif %}"
  style="--aic-opacity: {{ section.settings.opacity_overlay }}"
>
  <div class="aic__wrapper {% if section.settings.section_type == 'background' %}container{% endif %} {% if section.settings.section_type != 'background' %}color-{{ section.settings.color_scheme }}{% endif %} section-{{ section.id }}-padding">
    <div class="aic__info {% if section.settings.section_type != 'background' %}container{% endif %}">
      {%- if section.settings.icon != 'none' -%}
        <div
          class="
            aic__icon aic__icon--{{- section.settings.icon_size }} {% if section.settings.box_shadow %}aic__icon--shadow
            {% endif %} aic__icon--{{- section.settings.icon_style }} aic__icon--{{- section.settings.icon_fill_type }} color-{{ section.settings.icon_color_scheme }}
          "
        >
          <i
            class="icon icon-pack icon-{{ section.settings.icon }} {% if section.settings.icon_style == 'colored' and section.settings.icon_fill_type == 'gradient' %} text-gradient {% endif %}"
          ></i>
        </div>
      {%- endif -%}
      <div class="aic__text-wrapper">
        {% render 'section-text', section: section, class: 'aic' %}
        {% if section.settings.enable_subscribe %}
          {% render 'subscription',
            label: section.settings.button_label_subscribe,
            style: section.settings.button_style_subscribe,
            color_scheme: section.settings.color_scheme_subscribe,
            enable_box_shadow: section.settings.enable_box_shadow,
            class: 'aic'
          %}
        {% endif %}
      </div>
      {%- if section.settings.show_button or section.settings.show_button_2 -%}
        <div class="aic__buttons">
          {%- if section.settings.show_button and section.settings.button_label != blank -%}
            {% render 'button',
              link: section.settings.button_link,
              label: section.settings.button_label,
              style: section.settings.button_style,
              class: 'aic'
            %}
          {% endif %}
          {%- if section.settings.show_button_2 and section.settings.button_label_2 != blank -%}
            {% render 'button',
              link: section.settings.button_link_2,
              label: section.settings.button_label_2,
              style: section.settings.button_style_2,
              class: 'aic'
            %}
          {% endif %}
        </div>
      {% endif %}
    </div>

    <div class="aic__marquees">
      {% case section.settings.section_type %}
        {% when 'background' %}
          {% assign columns = section.settings.column_to_show %}
          {% assign counter = 1 %}
          {% assign limit = section.blocks.size | divided_by: columns %}
          {% comment %} For every column there's a single marquee {% endcomment %}
          {% for i in (1..columns) %}
            <div class="aic__marquee content" data-slides="{{ limit }}">
              {% comment %} Every marquee must start with a slide, not necessarily with the first index, but with a following one. {% endcomment %}
              {% for y in (counter..section.blocks.size) limit: limit %}
                {% for block in section.blocks %}
                  {% if forloop.index == y %}
                    {% render 'aic-item',
                      image: block.settings.image,
                      section: section,
                      section_index: section.index,
                      index: forloop.index,
                      block: block,
                      width: 745
                    %}
                  {% endif %}
                {% endfor %}
              {% endfor %}
            </div>
            {% comment %} E.g. counter = 1, limit = 3, so in the 1st marquee there will be 1st, 2nd, 3rd slides, and in the 2nd marquee there will be 4th, 5th, 6th slides, and so on {% endcomment %}
            {% assign counter = counter | plus: limit %}
          {% endfor %}
        {% when 'right' %}
          {% assign counter = 1 %}
          {% assign limit = section.blocks.size | divided_by: 2 %}
          {% comment %} For every column there's a single marquee {% endcomment %}
          {% for i in (1..2) %}
            <div class="aic__marquee content" data-slides="{{ limit }}">
              {% comment %} Every marquee must start with a slide, not necessarily with the first index, but with a following one. {% endcomment %}
              {% for y in (counter..section.blocks.size) limit: limit %}
                {% for block in section.blocks %}
                  {% if forloop.index == y %}
                    {% render 'aic-item',
                      image: block.settings.image,
                      section: section,
                      section_index: section.index,
                      index: forloop.index,
                      block: block,
                      width: 575
                    %}
                  {% endif %}
                {% endfor %}
              {% endfor %}
            </div>
            {% comment %} E.g. counter = 1, limit = 3, so in the 1st marquee there will be 1st, 2nd, 3rd slides, and in the 2nd marquee there will be 4th, 5th, 6th slides, and so on {% endcomment %}
            {% assign counter = counter | plus: limit %}
          {% endfor %}
        {% when 'bottom' %}
          <div class="aic__marquee-horizontal content" data-duplicated="true">
            {% for block in section.blocks %}
              {% render 'aic-item',
                image: block.settings.image,
                section: section,
                width: block.settings.image_width,
                block: block,
                index: forloop.index,
                section_index: section.index
              %}
            {% endfor %}
          </div>
      {% endcase %}
    </div>
  </div>
</div>

{%- if section.settings.header_overlay == true %}
  {% render 'section-overlay', section: section %}
{% endif -%}

