{{ 'component-subscription.css' | asset_url | stylesheet_tag }}
{{ 'section-hero.css' | asset_url | stylesheet_tag }}

{%- style -%}
  {% render 'section-padding' %}
{%- endstyle -%}

{%- liquid
  assign image_ratio = section.settings.image_ratio
-%}

{%- if image_ratio == 'adapt' and section.settings.image != blank -%}
  {% assign padding_bottom = 1 | divided_by: section.settings.image.aspect_ratio | times: 100 | round: 2 %}
  {% style %}
    #Hero-{{ section.id }} .hero__image-inner::before {
    	content: '';
    	display: block;
    	padding-bottom: {{ padding_bottom }}%;
    }

    #Hero-{{ section.id }} .hero__outer--overlay::before {
    	content: '';
    	display: block;
    	padding-bottom: {{ padding_bottom }}%;
    }
  {% endstyle %}
{%- elsif image_ratio == 'adapt' and section.settings.video != blank -%}
  {% assign padding_bottom = 1 | divided_by: section.settings.video.aspect_ratio | times: 100 | round: 2 %}
  <style>
    #Hero-{{ section.id }} .hero__image-inner::before {
    	content: '';
    	display: block;
    	padding-bottom: {{ padding_bottom }}%;
    }

    #Hero-{{ section.id }} .hero__outer--overlay::before {
    	content: '';
    	display: block;
    	padding-bottom: {{ padding_bottom }}%;
    }
  </style>
{% else %}
  {% liquid
    assign aspect_ratio = section.settings.image_ratio | split: '/'
    assign temp = aspect_ratio[0] | append: '.0'
    assign padding_bottom = aspect_ratio[1] | divided_by: temp | times: 100 | round: 2
  %}
  {% style %}
    #Hero-{{ section.id }} .hero__image-inner::before {
    	content: '';
    	display: block;
    	padding-bottom: {{ padding_bottom }}%;
    }

    #Hero-{{ section.id }} .hero__outer--overlay::before {
    	content: '';
    	display: block;
    	padding-bottom: {{ padding_bottom }}%;
    }
  {% endstyle %}
{%- endif -%}

{% if section.settings.overlay_opacity == 0 %}
  {% style %}
    [data-scheme="dark"] {
      #Hero-{{ section.id }} .hero__overlay {
        opacity: 0.4;
      }
    }

    @media (prefers-color-scheme: dark) {
      [data-scheme="auto"] {
        #Hero-{{ section.id }} .hero__overlay {
          opacity: 0.4;
        }
      }
    }
  {% endstyle %}
{% endif %}

<div
  id="Hero-{{ section.id }}"
  class="hero section-{{ section.id }}-padding {% if section.settings.layout == 'overlay' and section.settings.header_overlay %} section--has-overlay {% if section.settings.header_transparent %} section--header-light {% endif %} {% endif %}"
>
  <div class="{% if section.settings.layout == 'overlay' %}{% if section.settings.full_screen == false %}container{% endif %}{% else %}container{% endif %}">
    <div
      class="
        hero__outer hero__outer--{{ section.settings.layout }} {% if section.settings.layout == 'overlay' %}color-{{ section.settings.color_scheme }}{% endif %} {% if section.settings.layout == 'horizontal' %}hero__outer--{{ section.settings.block_type }}{% endif %}
        {% if section.settings.desktop_image_position == 'before' %}hero__outer--desktop-order{% endif %}
        {% if section.settings.mobile_image_position == 'before' %}hero__outer--mobile-order{% endif %}
        {% if section.settings.layout == 'overlay' %}hero__outer--mobile-image-{{ section.settings.mobile_image_overlay }} {% endif %}
      "
    >
      <div
        class="
          hero__content {% if section.settings.layout == 'overlay' %}container{% endif %}
          {% if section.settings.layout == 'horizontal' or section.settings.layout == 'overlay' %}hero__content--{{ section.settings.vertical_alignment }}{% endif %}
          {% if section.settings.layout == 'vertical' or section.settings.layout == 'overlay' %}hero__content--{{ section.settings.horizontal_alignment }}{% endif %}
        "
      >
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'subtitle' %}
              <div class="hero__subtitle {{ block.settings.subheading_style }}" {{ block.shopify_attributes }}>
                {% if block.settings.subheading_icon != 'none' %}
                  <i class="icon icon-pack icon-{{ block.settings.subheading_icon }}"></i>
                {% endif %}
                {{ block.settings.subheading }}
              </div>
            {% when 'title' %}
              {% if block.settings.heading_line == 'rounded' %}
                {% assign color = block.settings.line_color | remove: '#' %}
                {% style %}
                  #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-xs {
                    background-image: url('data:image/svg+xml,\
                    <svg xmlns="http://www.w3.org/2000/svg" width="78" height="14" viewBox="0 0 78 14" fill="none">\
                      <path d="M3 11C15.1361 4.50757 39.4887 -3.17051 75 11" stroke="%23{{ color }}" stroke-width="6" stroke-linecap="round"/>\
                    </svg>');
                  }

                  #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-s {
                    background-image: url('data:image/svg+xml,\
                    <svg xmlns="http://www.w3.org/2000/svg" width="172" height="14" viewBox="0 0 172 14" fill="none">\
                      <path d="M3 11C30.9803 4.50757 87.1267 -3.17051 169 11" stroke="%23{{ color }}" stroke-width="6" stroke-linecap="round"/>\
                    </svg>');
                  }

                  #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-m {
                    background-image: url('data:image/svg+xml,\
                    <svg xmlns="http://www.w3.org/2000/svg" width="321" height="14" viewBox="0 0 321 14" fill="none">\
                      <path d="M3 11C56.0952 4.50757 162.638 -3.17051 318 11" stroke="%23{{ color }}" stroke-width="6" stroke-linecap="round"/>\
                    </svg>');
                  }

                  #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-l {
                    background-image: url('data:image/svg+xml,\
                    <svg xmlns="http://www.w3.org/2000/svg" width="373" height="14" viewBox="0 0 373 14" fill="none">\
                      <path d="M3 11C64.8602 4.50757 188.991 -3.17051 370 11" stroke="%23{{ color }}" stroke-width="6" stroke-linecap="round"/>\
                    </svg>');
                  }

                  #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-xl {
                    background-image: url('data:image/svg+xml,\
                    <svg xmlns="http://www.w3.org/2000/svg" width="568" height="14" viewBox="0 0 568 14" fill="none">\
                      <path d="M3 11C97.7286 4.50757 287.814 -3.17051 565 11" stroke="%23{{ color }}" stroke-width="6" stroke-linecap="round"/>\
                    </svg>');
                  }

                  #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-xxl {
                    background-image: url('data:image/svg+xml,\
                    <svg xmlns="http://www.w3.org/2000/svg" width="714" height="14" viewBox="0 0 714 14" fill="none">\
                      <path d="M3 11C122.338 4.50757 361.805 -3.17051 711 11" stroke="%23{{ color }}" stroke-width="6" stroke-linecap="round"/>\
                    </svg>');
                  }

                  [data-scheme="dark"] {
                    #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-xs {
                      background-image: url('data:image/svg+xml,\
                      <svg xmlns="http://www.w3.org/2000/svg" width="78" height="14" viewBox="0 0 78 14" fill="none">\
                        <path d="M3 11C15.1361 4.50757 39.4887 -3.17051 75 11" stroke="%234A4A4C" stroke-width="6" stroke-linecap="round"/>\
                      </svg>');
                    }

                    #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-s {
                      background-image: url('data:image/svg+xml,\
                      <svg xmlns="http://www.w3.org/2000/svg" width="172" height="14" viewBox="0 0 172 14" fill="none">\
                        <path d="M3 11C30.9803 4.50757 87.1267 -3.17051 169 11" stroke="%234A4A4C" stroke-width="6" stroke-linecap="round"/>\
                      </svg>');
                    }

                    #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-m {
                      background-image: url('data:image/svg+xml,\
                      <svg xmlns="http://www.w3.org/2000/svg" width="321" height="14" viewBox="0 0 321 14" fill="none">\
                        <path d="M3 11C56.0952 4.50757 162.638 -3.17051 318 11" stroke="%234A4A4C" stroke-width="6" stroke-linecap="round"/>\
                      </svg>');
                    }

                    #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-l {
                      background-image: url('data:image/svg+xml,\
                      <svg xmlns="http://www.w3.org/2000/svg" width="373" height="14" viewBox="0 0 373 14" fill="none">\
                        <path d="M3 11C64.8602 4.50757 188.991 -3.17051 370 11" stroke="%234A4A4C" stroke-width="6" stroke-linecap="round"/>\
                      </svg>');
                    }

                    #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-xl {
                      background-image: url('data:image/svg+xml,\
                      <svg xmlns="http://www.w3.org/2000/svg" width="568" height="14" viewBox="0 0 568 14" fill="none">\
                        <path d="M3 11C97.7286 4.50757 287.814 -3.17051 565 11" stroke="%234A4A4C" stroke-width="6" stroke-linecap="round"/>\
                      </svg>');
                    }

                    #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-xxl {
                      background-image: url('data:image/svg+xml,\
                      <svg xmlns="http://www.w3.org/2000/svg" width="714" height="14" viewBox="0 0 714 14" fill="none">\
                        <path d="M3 11C122.338 4.50757 361.805 -3.17051 711 11" stroke="%234A4A4C" stroke-width="6" stroke-linecap="round"/>\
                      </svg>');
                    }
                  }

                  @media (prefers-color-scheme: dark) {
                   [data-scheme="auto"] {
                      #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-xs {
                        background-image: url('data:image/svg+xml,\
                        <svg xmlns="http://www.w3.org/2000/svg" width="78" height="14" viewBox="0 0 78 14" fill="none">\
                          <path d="M3 11C15.1361 4.50757 39.4887 -3.17051 75 11" stroke="%234A4A4C" stroke-width="6" stroke-linecap="round"/>\
                        </svg>');
                      }

                      #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-s {
                        background-image: url('data:image/svg+xml,\
                        <svg xmlns="http://www.w3.org/2000/svg" width="172" height="14" viewBox="0 0 172 14" fill="none">\
                          <path d="M3 11C30.9803 4.50757 87.1267 -3.17051 169 11" stroke="%234A4A4C" stroke-width="6" stroke-linecap="round"/>\
                        </svg>');
                      }

                      #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-m {
                        background-image: url('data:image/svg+xml,\
                        <svg xmlns="http://www.w3.org/2000/svg" width="321" height="14" viewBox="0 0 321 14" fill="none">\
                          <path d="M3 11C56.0952 4.50757 162.638 -3.17051 318 11" stroke="%234A4A4C" stroke-width="6" stroke-linecap="round"/>\
                        </svg>');
                      }

                      #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-l {
                        background-image: url('data:image/svg+xml,\
                        <svg xmlns="http://www.w3.org/2000/svg" width="373" height="14" viewBox="0 0 373 14" fill="none">\
                          <path d="M3 11C64.8602 4.50757 188.991 -3.17051 370 11" stroke="%234A4A4C" stroke-width="6" stroke-linecap="round"/>\
                        </svg>');
                      }

                      #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-xl {
                        background-image: url('data:image/svg+xml,\
                        <svg xmlns="http://www.w3.org/2000/svg" width="568" height="14" viewBox="0 0 568 14" fill="none">\
                          <path d="M3 11C97.7286 4.50757 287.814 -3.17051 565 11" stroke="%234A4A4C" stroke-width="6" stroke-linecap="round"/>\
                        </svg>');
                      }

                      #Hero-{{ section.id }} .hero__title--rounded span[style*="text-decoration:underline"].title--rounded-xxl {
                        background-image: url('data:image/svg+xml,\
                        <svg xmlns="http://www.w3.org/2000/svg" width="714" height="14" viewBox="0 0 714 14" fill="none">\
                          <path d="M3 11C122.338 4.50757 361.805 -3.17051 711 11" stroke="%234A4A4C" stroke-width="6" stroke-linecap="round"/>\
                        </svg>');
                      }
                    }
                  }
                {% endstyle %}
              {% endif %}
              <h2
                class="
                  hero__title title {{ block.settings.heading_size }} hero__title--{{ block.settings.heading_line }}
                  title--{{ block.settings.heading_line }} js-anim-{{ block.settings.heading_animation }}
                  {% if block.settings.word_break and block.settings.heading_line != 'none' %} hero__title--word_break {% endif %}
                  {% if block.settings.word_break_an and block.settings.heading_animation != 'none' %} hero__title--word_break_an {% endif %}
                "
                data-content="{{ block.settings.heading | escape }}"
                {{ block.shopify_attributes }}
                style="--color-line: {{ block.settings.line_color }}"
              >
                {{ block.settings.heading }}
              </h2>
            {% when 'description' %}
              <div class="hero__description description body-large" {{ block.shopify_attributes }}>
                {{ block.settings.description }}
              </div>
            {% when 'subscription' %}
              <div class="hero__subscription-wrapper" {{ block.shopify_attributes }}>
                {% render 'subscription',
                  label: block.settings.subscription_button_label,
                  style: block.settings.subscription_button_style,
                  color_scheme: block.settings.color_scheme_subscribe,
                  enable_box_shadow: block.settings.enable_box_shadow,
                  class: 'hero'
                %}
              </div>
            {% when 'buttons' %}
              <div class="hero__buttons" {{ block.shopify_attributes }}>
                {% if block.settings.show_button_1 != blank %}
                  {% render 'button',
                    link: block.settings.button_link_1,
                    label: block.settings.button_label_1,
                    style: block.settings.button_style_1
                  %}
                {% endif %}
                {% if block.settings.show_button_2 != blank %}
                  {% render 'button',
                    link: block.settings.button_link_2,
                    label: block.settings.button_label_2,
                    style: block.settings.button_style_2
                  %}
                {% endif %}
              </div>
            {% when 'captions' %}
              {% if block.settings.show_caption_1 or block.settings.show_caption_2 or block.settings.show_caption_3 %}
                <div class="hero__captions" {{ block.shopify_attributes }}>
                  {% if block.settings.show_caption_1 %}
                    <div class="hero__captions-item">
                      {% if block.settings.caption_icon_1 != 'none' %}
                        <i class="icon icon-pack icon-{{ block.settings.caption_icon_1 }}"></i>
                      {% endif %}
                      <span class="body-small">{{ block.settings.caption_text_1 }}</span>
                    </div>
                  {% endif %}
                  {% if block.settings.show_caption_2 %}
                    <div class="hero__captions-item">
                      {% if block.settings.caption_icon_2 != 'none' %}
                        <i class="icon icon-pack icon-{{ block.settings.caption_icon_2 }}"></i>
                      {% endif %}
                      <span class="body-small">{{ block.settings.caption_text_2 }}</span>
                    </div>
                  {% endif %}
                  {% if block.settings.show_caption_3 %}
                    <div class="hero__captions-item">
                      {% if block.settings.caption_icon_3 != 'none' %}
                        <i class="icon icon-pack icon-{{ block.settings.caption_icon_3 }}"></i>
                      {% endif %}
                      <span class="body-small">{{ block.settings.caption_text_3 }}</span>
                    </div>
                  {% endif %}
                </div>
              {% endif %}
            {% when 'users' %}
              <div class="hero__users" {{ block.shopify_attributes }}>
                {% if block.settings.user_image_1 != blank %}
                  <div class="hero__users-image">
                    {{
                      block.settings.user_image_1
                      | image_url: width: block.settings.user_image_1.width
                      | image_tag: loading: 'lazy', widths: '70, 140'
                    }}
                  </div>
                {% endif %}

                {% if block.settings.user_image_2 != blank %}
                  <div class="hero__users-image">
                    {{
                      block.settings.user_image_2
                      | image_url: width: block.settings.user_image_2.width
                      | image_tag: loading: 'lazy', widths: '70, 140'
                    }}
                  </div>
                {% endif %}

                {% if block.settings.user_image_3 != blank %}
                  <div class="hero__users-image">
                    {{
                      block.settings.user_image_3
                      | image_url: width: block.settings.user_image_3.width
                      | image_tag: loading: 'lazy', widths: '70, 140'
                    }}
                  </div>
                {% endif %}

                {% if block.settings.user_image_4 != blank %}
                  <div class="hero__users-image">
                    {{
                      block.settings.user_image_4
                      | image_url: width: block.settings.user_image_4.width
                      | image_tag: loading: 'lazy', widths: '70, 140'
                    }}
                  </div>
                {% endif %}

                {% if block.settings.user_image_5 != blank %}
                  <div class="hero__users-image">
                    {{
                      block.settings.user_image_5
                      | image_url: width: block.settings.user_image_5.width
                      | image_tag: loading: 'lazy', widths: '70, 140'
                    }}
                  </div>
                {% endif %}

                {% if block.settings.user_text != blank %}
                  <span class="body-small">{{ block.settings.user_text }}</span>
                {% endif %}
              </div>
          {% endcase %}
        {% endfor %}
      </div>
      {% if section.settings.video != blank %}
        <div class="hero__image">
          <div class="hero__image-inner hero__image-inner--{{ section.settings.object_fit }}">
            {% if section.settings.layout == 'overlay' %}
              <span class="hero__overlay" style="--overlay-opacity: {{ section.settings.overlay_opacity }};"></span>
            {% endif %}
            {%- capture style -%}object-fit: {{ section.settings.object_fit }};{%- endcapture -%}
            {{
              section.settings.video
              | video_tag: image_size: '1920x', autoplay: true, loop: true, muted: true, controls: false, style: style
            }}
          </div>
        </div>
      {% elsif section.settings.image != blank %}
        <div class="hero__image">
          <div class="hero__image-inner">
            {% if section.settings.layout == 'overlay' %}
              <span class="hero__overlay" style="--overlay-opacity: {{ section.settings.overlay_opacity }};"></span>
            {% endif %}

            {% liquid
              assign width_ratio = 1
              if image_ratio != 'adapt'
                assign aspect_ratio_img = section.settings.image.aspect_ratio | round: 2
                assign aspect_ratio_settings = aspect_ratio[0] | plus: 0.0 | divided_by: aspect_ratio[1] | round: 2

                if aspect_ratio_img > aspect_ratio_settings
                  assign width_ratio = aspect_ratio_img | divided_by: aspect_ratio_settings | round: 2
                endif
              endif

              if section.settings.image.width > section.settings.image.height
                assign width_adapt_ratio = 320 | times: section.settings.image.aspect_ratio | round: 2
              endif
            %}

            {% capture sizes %}
              {% if section.settings.layout == 'horizontal' %}
                (min-width: 1200px) calc((40vw - 40px) * {{ width_ratio }}),
                (min-width: 990px) calc((50vw - 40px) * {{ width_ratio }}),
              {% else %}
                (min-width: 990px) calc((100vw - 40px) * {{ width_ratio }}),
              {% endif %}
              {% if section.settings.image.width > section.settings.image.height %}
                {{ width_adapt_ratio }}px
              {% else %}
                calc((100vw - 40px) * {{ width_ratio }})
              {% endif %}
            {% endcapture %}

            {% liquid
              assign height = section.settings.image.height
              assign width = section.settings.image.width
            %}

            {%- capture style -%}object-fit: {{ section.settings.object_fit }};{%- endcapture -%}

            {{
              section.settings.image
              | image_url: width: section.settings.image.width
              | image_tag:
                loading: 'lazy',
                sizes: sizes,
                widths: '360, 533, 720, 940, 1066, 1200, 1400, 1600, 1800, 2000, 2200, 2400, 2600, 2800, 3000, 3264, 3400, 3584, 3800, 4000, 4200, 4400, 4600, 4800, 5000, 5200, 5400, 5600',
                height: height,
                width: width,
                style: style
            }}
          </div>
        </div>

      {% else %}
        <div class="hero__image">
          <div class="hero__image-inner hero__image-inner--{{ section.settings.object_fit }}">
            {% if section.settings.layout == 'overlay' %}
              <span class="hero__overlay" style="--overlay-opacity: {{ section.settings.overlay_opacity }};"></span>
            {% endif %}
            {{ 'hero-apparel-1' | placeholder_svg_tag: 'placeholder-svg' }}
          </div>
        </div>
      {% endif %}
    </div>
  </div>
</div>

{%- if section.settings.header_overlay == true %}
  {% render 'section-overlay', section: section %}
{% endif -%}

{% schema %}
{
  "name": "t:sections.hero.name",
  "tag": "section",
  "class": "section-hero spaced-section",
  "enabled_on": {
    "templates": ["*"]
  },
  "settings": [
    {
      "type": "select",
      "id": "layout",
      "options": [
        {
          "value": "horizontal",
          "label": "t:sections.hero.settings.layout.options__1.label"
        },
        {
          "value": "vertical",
          "label": "t:sections.hero.settings.layout.options__2.label"
        },
        {
          "value": "overlay",
          "label": "t:sections.hero.settings.layout.options__3.label"
        }
      ],
      "default": "horizontal",
      "label": "t:sections.hero.settings.layout.label"
    },
    {
      "type": "checkbox",
      "id": "header_overlay",
      "default": false,
      "label": "t:sections.all.header_overlay.label",
      "info": "t:sections.hero.settings.header_overlay.info"
    },
    {
      "type": "checkbox",
      "id": "header_transparent",
      "label": "t:sections.all.header_transparent.label",
      "info": "t:sections.all.header_transparent.info",
      "default": false
    },
    {
      "type": "select",
      "id": "block_type",
      "options": [
        {
          "value": "plain",
          "label": "t:sections.hero.settings.block_type.options__1.label"
        },
        {
          "value": "subtle",
          "label": "t:sections.hero.settings.block_type.options__2.label"
        }
      ],
      "default": "subtle",
      "label": "t:sections.hero.settings.block_type.label",
      "info": "t:sections.hero.settings.block_type.info"
    },
    {
      "type": "color_scheme",
      "id": "color_scheme",
      "default": "background-3",
      "label": "t:sections.all.color_scheme.label",
      "info": "t:sections.hero.settings.color_scheme.info"
    },
    {
      "type": "checkbox",
      "id": "full_screen",
      "default": false,
      "label": "t:sections.hero.settings.full_screen.label",
      "info": "t:sections.hero.settings.full_screen.info"
    },
    {
      "type": "header",
      "content": "t:sections.hero.settings.header__1.content"
    },
    {
      "type": "image_picker",
      "id": "image",
      "label": "t:sections.all.image.label"
    },
    {
      "type": "checkbox",
      "id": "show_video",
      "label": "t:sections.all.video.show_video.label",
      "default": false
    },
    {
      "type": "video",
      "id": "video",
      "label": "t:sections.all.video.video.label"
    },
    {
      "type": "range",
      "id": "overlay_opacity",
      "min": 0,
      "max": 1,
      "step": 0.1,
      "default": 0.5,
      "label": "t:sections.all.overlay_opacity.label",
      "info": "t:sections.hero.settings.overlay_opacity.info"
    },
    {
      "type": "select",
      "id": "image_ratio",
      "options": [
        {
          "value": "1/1",
          "label": "t:sections.image-banner.settings.image_ratio.options__1.label"
