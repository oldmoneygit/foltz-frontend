{{ 'section-social-media.css' | asset_url | stylesheet_tag }}
<script src="{{ 'social-media.js' | asset_url }}" defer="defer"></script>

{%- style -%}
  {% render 'section-padding' %}
  {% render 'section-margin' %}
{%- endstyle -%}

{% liquid
  assign aspect_ratio = section.settings.image_ratio | split: '/'
  assign temp = aspect_ratio[0] | append: '.0'
  assign padding_bottom = aspect_ratio[1] | divided_by: temp | times: 100 | round: 2
%}

<div class="social-media color-{{ section.settings.color_scheme }} background section-{{ section.id }}-padding">
  <div class="container">
    {% render 'section-text', section: section, class: 'social-media', text_align: section.settings.text_align %}

    <div class="content social-media__sliders social-media__sliders--{{ section.settings.column_to_show }}">
      {%- style -%}
        .section-{{ section.id }}-padding .social-media__image::before,
        .section-{{ section.id }}-padding .social-media__info::before {
        	content: "";
        	display: block;
        	padding-bottom: {{ padding_bottom }}%;
        }
      {%- endstyle -%}

      {%- for block in section.blocks -%}
        {% render 'social-media-item', section: section, block: block, padding_bottom: padding_bottom %}
      {%- endfor -%}
    </div>
  </div>
</div>

