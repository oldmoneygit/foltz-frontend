{{ 'section-pricing-table.css' | asset_url | stylesheet_tag }}

{%- style -%}
  {% render 'section-padding' %}
  {% render 'section-margin' %}
{%- endstyle -%}


<div class="pricing-table color-{{ section.settings.color_scheme }} background section-{{ section.id }}-padding 
  {% if section.settings.enable_border %} section-border {% if section.settings.border_full %} section-border-full {% endif %} {% endif %}">
  <div class="container">
    {% render 'section-text', 
      section: section, 
      class: 'pricing-table', 
      text_align: section.settings.text_align 
    %}
    <div class="content pricing-table__content">
      <div class="pricing-table__items">
        {% for block in section.blocks %}
          <div class="pricing-table__item color-{{ block.settings.color_scheme }}
            {% if block.settings.show_shadow %} pricing-table__item_shadow {% endif %}
            pricing-table__item--{{ block.type }}" {{ block.shopify_attributes }}>
            <div class="pricing-table__item-header">
              {% if block.settings.badge_text != blank %}
                <span class="pricing-table__badge subtitle subtitle--secondary" style="background-color: {{ block.settings.badge_bg }}; color: {{ block.settings.badge_color }}">
                  {% if block.settings.icon != 'none' %}
                    <i class="icon icon-pack icon-{{ block.settings.badge_icon }}"></i>
                  {% endif %}
                  {{ block.settings.badge_text }}
                </span>
              {% endif %}
              {% if block.settings.heading != blank %}
                <h3 class="pricing-table__item-title h5 {% if block.settings.badge_text != blank %} pricing-table__item-title_with-badge {% endif %}">
                  {{ block.settings.heading }}
                </h3>
              {% endif %}
              {% if block.settings.heading_price != blank %}
                <div class="pricing-table__item-price {{ section.settings.heading_size_price }}
                  {% if block.type == 'custom' %} accent {% endif %}
                  {% if block.settings.color_scheme.settings.color_accent_gradient != empty and block.type == 'custom' %} text-gradient {% endif %}">
                  {{ block.settings.heading_price }}
                </div>
              {% endif %}
              {% if block.settings.description != blank %}
                <p class="pricing-table__item-description">
                  {{ block.settings.description }}
                </p>
              {% endif %}
            </div>

            {% if block.type == 'price' and block.settings.features_list != blank %}
              <div class="pricing-table__item-list">
                {{ block.settings.features_list }}
              </div>
            {% endif %}

              {% if block.type == 'custom' %}
                {% if block.settings.icon != 'none' or block.settings.add_heading != blank or block.settings.add_description != blank %}
                  <div class="pricing-table__custom-content">
                    {% if block.settings.icon != 'none' %}
                      <div class="pricing-table__icon
                        pricing-table__icon--{{- block.settings.icon_size }} 
                        pricing-table__icon--{{- block.settings.icon_style }}
                        pricing-table__icon--{{- block.settings.icon_fill_type }}">
                        <i class="icon icon-pack icon-{{ block.settings.icon }} {% if block.settings.icon_style == 'colored' and block.settings.icon_fill_type == 'gradient' %} text-gradient {% endif %}"></i>
                      </div>
                    {% endif %}
                    {% if block.settings.add_heading != blank %}
                      <div class="pricing-table__add-heading h5">
                        {{ block.settings.add_heading }}
                      </div>
                    {% endif %}
                    {% if block.settings.add_description != blank %}
                      <div class="pricing-table__add-description">
                        {{ block.settings.add_description }}
                      </div>
                    {% endif %}
                  </div>
                {% endif %}
              {% endif %}

            {%- if block.settings.show_button -%}
              {% render 'button',
                link: block.settings.button_link,
                label: block.settings.button_label,
                style: block.settings.button_style,
                class: 'pricing-table'
              %}
            {% endif %}
          </div>
        {% endfor %}
      </div>
    </div>
  </div>
</div>


